Xcursor.theme: DMZ-White

URxvt.font:             xft:JetBrainsMono Nerd Font Mono:pixelsize=13
URxvt.boldFont:         xft:JetBrainsMono Nerd Font Mono:pixelsize=13:bold
URxvt.italicFont:       xft:JetBrainsMono Nerd Font Mono:pixelsize=13:italic
URxvt.boldItalicFont:   xft:JetBrainsMono Nerd Font Mono:pixelsize=13:bold:italic
!URxvt.letterSpace:     -1

*background: #000000
*foreground: #8AE234
! black
*color0: #2E3436
*color8: #555753
! red
*color1: #CC0000
*color9: #EF2929
! green
*color2: #4E9A06
*color10: #8AE234
! yellow
*color3: #C4A000
*color11: #FCE94F
! blue
*color4: #3465A4
*color12: #729FCF
! magenta
*color5: #75507B
*color13: #AD7FA8
! cyan
*color6: #06989A
*color14: #34E2E2
! white
*color7: #D3D7CF
*color15: #EEEEEC

URxvt.internalBorder:      0
URxvt.externalBorder:      0

URxvt.scrollBar:           false

URxvt*fading:              30

URxvt.transparent:         true
URxvt.shading:             10

Rxvt.saveLines:            200000

URxvt.perl-ext-common:     matcher,resize-font,clipboard

URxvt.resize-font.smaller: C-Down
URxvt.resize-font.bigger:  C-Up

URxvt.url-launcher:        /usr/sbin/brave
URxvt.keysym.M-Delete:     perl:matcher:list
URxvt.colorUL:             #7780a1

URxvt.keysym.Control-Left:  \033[1;5D
URxvt.keysym.Control-Right: \033[1;5C

